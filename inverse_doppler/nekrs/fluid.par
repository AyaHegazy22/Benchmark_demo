[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.9
  polynomialOrder = 1
  writeControl = timeStep
  writeInterval = 10
  extrapolation = subCycling

[VELOCITY]
  boundaryTypeMap = w, w, w, w, w, w
  residualTol = 1.0e-6
  residualProj = false 
  solver = none
[PRESSURE]
  residualTol = 1.0e-6
  residualProj = false
[TEMPERATURE]
  conductivity = 0.006
  rhoCp = 1.0
  boundaryTypeMap = t, I, I, I, I, I
  residualTol = 1.0e-3
